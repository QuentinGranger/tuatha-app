{
  "name": "tuatha-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_TELEMETRY_DISABLED=1 next dev --turbo",
    "dev:stable": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:clean": "rm -rf .next && NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:safe": "rm -rf .next && NODE_NO_WARNINGS=1 NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096 --trace-warnings' next dev",
    "dev:prod": "yarn build && yarn start",
    "shazam": "bash ./custom-dev.sh",
    "alohomora": "bash ./custom-dev.sh",
    "avada-kedavra": "bash ./avada-kedavra.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "prisma:seed": "node prisma/seed.mjs",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate && npm run prisma:seed"
  },
  "prisma": {
    "seed": "node prisma/seed.mjs"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "bcrypt": "^5.1.1",
    "chart.js": "^4.4.8",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "express": "^4.21.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "next": "15.1.6",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "openai": "^4.87.3",
    "prisma": "^4.16.2",
    "puppeteer": "^24.4.0",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.11.5",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
